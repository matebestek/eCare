definition
	CLUSTER[at0000] matches {	-- Test ravnotezje
		items cardinality matches {0..*; unordered} matches {
			CLUSTER[at0003] occurrences matches {0..1} matches {	-- Testi na tenzimetrijski plošči
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Dolžina nihanja telesa v mirovanju
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"cm">
										precision = <|1|>
									>
								>
							>
						}
					}
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Trajanje testa na tenziometrijski plošči
						value matches {
							DV_TIME matches {*}
						}
					}
				}
			}
		}
	}

