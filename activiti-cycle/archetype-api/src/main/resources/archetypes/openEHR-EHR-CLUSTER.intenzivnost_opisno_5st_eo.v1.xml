  <ontology>
    <term_definitions language="sl">
      <items code="at0000">
        <items id="text">Intenzivnost opisno 5st eo</items>
        <items id="description">unknown</items>
        <items id="comment" />
      </items>
      <items code="at0001">
        <items id="text">Intenzivnost vadbe (5 stopenj)</items>
        <items id="description">*</items>
        <items id="comment" />
      </items>
      <items code="at0002">
        <items id="text">Zelo nizka intenzivnost</items>
        <items id="description">Regeneracija (do 60 % FSmax). Med aktivnostjo je mogoč sproščen pogovor.</items>
        <items id="comment" />
      </items>
      <items code="at0003">
        <items id="text">Nizka intenzivnost</items>
        <items id="description">Aerobno I (60 do 70 % FSmax). Med aktivnostjo je mogoč pogovor s krajšimi prekinitvami.</items>
        <items id="comment" />
      </items>
      <items code="at0004">
        <items id="text">Zmerna intenzivnost</items>
        <items id="description">Aerobno II (70 do 80 % FSmax). Med vadbo je pogovor otežen, pogovarja se lahko le v zelo kratkih stavkih.</items>
        <items id="comment" />
      </items>
      <items code="at0005">
        <items id="text">Visoka intenzivnost</items>
        <items id="description">Aerobno in anaerobno (80 do 90 % FSmax). Med vadbo je pogovor otežen, pogovarja se lahko le v zelo kratkih stavkih.</items>
        <items id="comment" />
      </items>
      <items code="at0006">
        <items id="text">Zelo visoka intenzivnost</items>
        <items id="description">Pretežno anaerobno (90 do 100 % FSmax). Med vadbo pogovor praktično ni mogoč.</items>
        <items id="comment" />
      </items>
    </term_definitions>
  </ontology>
