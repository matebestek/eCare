															at0069, 	-- Forsirani izdihani volumen v 6 sek (FEV6)
															at0064, 	-- Počasna vitalna kapaciteta (SVC/EVC)
															at0066, 	-- Celotna pljučna kapaciteta (TLC)
															at0076, 	-- Inspiratorna kapaciteta (IC/IVC)
															at0077, 	-- Ekspiratorni rezervni volumen (ERV)
															at0078, 	-- Inspiratorni rezervni volumen (IRV)
															at0082, 	-- Dihalni volumen
															at0083, 	-- Rezidualni volumen (RV)
															at0084]	-- Funkcionalna rezidualna kapaciteta (FRC)
														}
													}
												}
											}
											ELEMENT[at0054] occurrences matches {0..1} matches {	-- Predviden
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::129]>
														list = <
															["1"] = <
																units = <"l">
																magnitude = <|>=0.0|>
															>
															["2"] = <
																units = <"ml">
																magnitude = <|>=0.0|>
															>
														>
													>
												}
											}
											ELEMENT[at0053] occurrences matches {0..1} matches {	-- Izmerjen
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::129]>
														list = <
															["1"] = <
																units = <"l">
																magnitude = <|>=0.0|>
															>
															["2"] = <
																units = <"ml">
																magnitude = <|>=0.0|>
															>
														>
													>
												}
											}
											ELEMENT[at0044] occurrences matches {0..1} matches {	-- Razmerje izmerjen/predviden
												value matches {
													DV_PROPORTION matches {
														numerator matches {|>=0.0|}
														denominator matches {|0.0..100.0|}
														type matches {0, 2}
													}
												}
											}
										}
									}
									CLUSTER[at0057] occurrences matches {0..*} matches {	-- PEF
										items cardinality matches {1..*; ordered} matches {
											ELEMENT[at0088] occurrences matches {0..1} matches {	-- Ime testa
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0071, 	-- *Peak expiratory flow (PEF)(en)
															at0072, 	-- *Forced expiratory flow 25-75% (FEF25-75%)(en)
															at0073, 	-- *Forced expiratory flow 25-50% (FEF25-50%)(en)
															at0074, 	-- *Forced inspratory flow 25-75% (FIF25-75%)(en)
															at0075, 	-- *Forced inspratory flow 25-50% (FIF25-50%)(en)
															at0079]	-- *Maximum voluntary ventilation(en)
														}
													}
												}
											}
											ELEMENT[at0008] occurrences matches {0..1} matches {	-- Predviden
												value matches {
