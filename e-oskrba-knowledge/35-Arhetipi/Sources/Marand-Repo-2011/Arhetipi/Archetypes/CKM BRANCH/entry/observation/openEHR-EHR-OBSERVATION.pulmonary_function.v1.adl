											ELEMENT[at0087] occurrences matches {0..1} matches {	-- Ime testa
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0065, 	-- Forsirana vitalna kapaciteta (FVC)
															at0067, 	-- Forsirani izdihani volumen v prvi sekundi (FEV1)
															at0070, 	-- Forsirani izdihani volumen v .75 sek (FEV.75)
