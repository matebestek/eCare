  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Fluid output</items>
        <items id="description">Recording of findings and measurement (or estimation) of bodily output such as fluid output and defaecation.</items>
        <items id="comment" />
      </items>
      <items code="at0001">
        <items id="text">Event Series</items>
        <items id="description">@ internal @</items>
        <items id="comment" />
      </items>
      <items code="at0002">
        <items id="text">Any event</items>
        <items id="description">The estimation of fluid output without specifying the period of time.</items>
        <items id="comment" />
      </items>
      <items code="at0003">
        <items id="text">List</items>
        <items id="description">@ internal @</items>
        <items id="comment" />
      </items>
      <items code="at0006">
        <items id="text">Total</items>
        <items id="description">For recording total fluid output over a period (may transcend a number of interval or point in time records).</items>
        <items id="comment" />
      </items>
      <items code="at0007">
        <items id="text">Actual</items>
        <items id="description">For recording the fluid output over a period of time when this will later be totalled.</items>
        <items id="comment" />
      </items>
      <items code="at0008">
        <items id="text">List</items>
        <items id="description">@ internal @</items>
        <items id="comment" />
      </items>
      <items code="at0016">
        <items id="text">Fluid detail</items>
        <items id="description">Details of the fluid or substance observed being observed.</items>
        <items id="comment" />
      </items>
      <items code="at0018">
        <items id="text">Estimated</items>
        <items id="description">The fluid volume has been estimated.</items>
        <items id="comment" />
      </items>
      <items code="at0019">
        <items id="text">Measured</items>
        <items id="description">The fluid volume has been directly measured.</items>
        <items id="comment" />
      </items>
      <items code="at0020">
        <items id="text">Stated</items>
        <items id="description">The fluid output has been stated.</items>
        <items id="comment" />
      </items>
      <items code="at0028">
        <items id="text">Measurement device </items>
        <items id="description">Details of device used to measure the fluid output.</items>
        <items id="comment" />
      </items>
      <items code="at0031">
        <items id="text">Measurement precision</items>
        <items id="description">Records the level of precision of the fluid output observation.</items>
        <items id="comment" />
      </items>
    </term_definitions>
  </ontology>
