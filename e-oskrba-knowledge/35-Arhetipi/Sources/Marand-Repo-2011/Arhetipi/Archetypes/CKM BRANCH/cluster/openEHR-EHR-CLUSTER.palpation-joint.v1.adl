									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0003, 	-- *Superficial(en)
											at0004]	-- *Deep(en)
										}
									}
								}
							}
							ELEMENT[at0020] occurrences matches {0..1} matches {	-- *Degree(en)
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0006, 	-- *absent(en)
											at0021, 	-- *mild(en)
											at0022, 	-- *moderate(en)
											at0023, 	-- *marked(en)
											at0024]	-- *extreme(en)
