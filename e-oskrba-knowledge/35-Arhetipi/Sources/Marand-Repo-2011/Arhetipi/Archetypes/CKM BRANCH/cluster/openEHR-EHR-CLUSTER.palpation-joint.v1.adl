									archetype_id/value matches {/.*/}
							}
							ELEMENT[at0081] occurrences matches {0..1} matches {	-- *Cough impulse(en)
								value matches {
									DV_BOOLEAN matches {
										value matches {True}
									}
								}
							}
						}
					}
					CLUSTER[at0053] occurrences matches {0..1} matches {	-- *Surface/Border(en)
