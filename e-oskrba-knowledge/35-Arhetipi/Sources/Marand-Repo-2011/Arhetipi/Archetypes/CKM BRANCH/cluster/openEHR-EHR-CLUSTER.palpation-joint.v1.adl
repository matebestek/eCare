							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0013, 	-- *Normal(en)
									at0014, 	-- *Probably normal(en)
									at0015, 	-- *No abnormality detected(en)
									at0060, 	-- *Soft(en)
									at0061, 	-- *No tenderness(en)
									at0068, 	-- *No masses(en)
									at0069, 	-- *No foreign bodies(en)
									at0082]	-- *No hernias(en)
								}
							}
						}
					}
				}
			}
			ELEMENT[at0001] occurrences matches {0..1} matches {	-- *Clinical description(en)
				value matches {
					DV_TEXT matches {*}
				}
			}
			CLUSTER[at0080] occurrences matches {0..1} matches {	-- Ugotovitve
