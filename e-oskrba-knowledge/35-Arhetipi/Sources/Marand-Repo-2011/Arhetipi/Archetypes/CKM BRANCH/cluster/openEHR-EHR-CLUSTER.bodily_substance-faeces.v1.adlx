							}
						}
					}
					ELEMENT[at0.87] occurrences matches {0..1} matches {	-- Bristol stool scale
						value matches {
							1|[local::at0.88], 	-- Separate hard lumps, like nuts (hard to pass).
							2|[local::at0.90], 	-- Sausage-shaped, but lumpy.
							3|[local::at0.91], 	-- Like a sausage but with cracks on its surface.
							4|[local::at0.92], 	-- Like a sausage or snake, smooth and soft
							5|[local::at0.93], 	-- Soft blobs with clear cut edges (passed easily).
							6|[local::at0.94], 	-- Fluffy pieces with ragged edges, a mushy stool.
							7|[local::at0.95]  	-- Watery, no solid pieces. Entirely liquid.
						}
					}
				}
			}
			CLUSTER[at0032] occurrences matches {0..1} matches {	-- Contained substances
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0089] occurrences matches {0..1} matches {	-- Description
