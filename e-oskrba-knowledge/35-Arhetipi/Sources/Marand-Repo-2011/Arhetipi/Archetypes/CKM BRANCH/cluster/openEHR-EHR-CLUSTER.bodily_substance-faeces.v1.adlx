							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0049, 	-- Fresh
									at0050]	-- Old
								}
							}
						}
					}
					ELEMENT[at0014] occurrences matches {0..1} matches {	-- Turbidity
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0015, 	-- Clear
									at0016, 	-- Slightly cloudy
									at0017, 	-- Cloudy
									at0018, 	-- Opaque
									at0019]	-- Flocculent
								}
							}
						}
					}
					ELEMENT[at0020] occurrences matches {0..1} matches {	-- Viscosity
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0021, 	-- Watery
									at0022, 	-- Oily/viscous
									at0023, 	-- Creamy/sticky
									at0024, 	-- Thick
									at0063, 	-- Waxy
									at0082]	-- Particulate
								}
							}
						}
					}
					ELEMENT[at0037] occurrences matches {0..1} matches {	-- Sediment
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0038, 	-- Scant
									at0039, 	-- Moderate
									at0040, 	-- Profuse
