definition
	EVALUATION[at0000.1] matches {	-- *Infection related problem(en)
		data matches {
			ITEM_TREE[at0001] matches {	-- *structure(en)
				items cardinality matches {1..*; ordered} matches {
					ELEMENT[at0002.1] occurrences matches {0..1} matches {	-- Vrsta
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0.77, 	-- Norice
									at0.78, 	-- Škrlatinka
									at0.79, 	-- Oslovski kašelj
									at0.80, 	-- Davica
									at0.81, 	-- Tuberkuloza
									at0.82, 	-- Akutni gastroenterokolitis
									at0.83, 	-- Respiratorne okužbe
									at0.84, 	-- Vbod klopa
									at0.85, 	-- Stik z živalmi
									at0.86, 	-- Potovanje v tujino
									at0.91]	-- Drugo
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {	-- *Clinical description(en)
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0010] matches {	-- *Date of onset(en)
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0037] occurrences matches {0..1} matches {	-- *Age of onset(en)
						value matches {
							DV_DURATION matches {*}
						}
					}
					allow_archetype CLUSTER[at0044] occurrences matches {0..*} matches {	-- *Anatomical location(en)
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1/}
					}
					CLUSTER[at0018] occurrences matches {0..1} matches {	-- *Occurrence Summary(en)
						items cardinality matches {1..*; ordered} matches {
							ELEMENT[at0025] occurrences matches {0..1} matches {	-- *Number(en)
								value matches {
									DV_COUNT matches {
										magnitude matches {|>=1|}
									}
								}
							}
							ELEMENT[at0019] occurrences matches {0..1} matches {	-- *Frequency(en)
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::382]>
										list = <
											["1"] = <
												units = <"/d">
											>
											["2"] = <
												units = <"/yr">
											>
											["3"] = <
												units = <"/h">
											>
											["4"] = <
												units = <"/wk">
											>
											["5"] = <
												units = <"/mo">
											>
											["6"] = <
												units = <"/min">
											>
										>
									>
								}
							}
							ELEMENT[at0020] occurrences matches {0..1} matches {	-- *Last occurrence(en)
								value matches {
									DV_DATE matches {*}
								}
							}
							ELEMENT[at0072] occurrences matches {0..1} matches {	-- *Description(en)
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0073] occurrences matches {0..*} matches {	-- *Link to previous occurrence(en)
								value matches {
									DV_URI matches {*}
								}
							}
						}
					}
					ELEMENT[at0015] occurrences matches {0..*} matches {	-- *Causative Agent(en)
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0.87] occurrences matches {0..1} matches {	-- Stik z boleznijo
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0.88, 	-- Vrtec
									at0.89, 	-- Šola
									at0.90]	-- Družinski član
								}
							}
						}
					}
					CLUSTER[at0026] occurrences matches {0..*} matches {	-- *Related items(en)
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0028] matches {	-- *Related item(en)
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0056] occurrences matches {0..1} matches {	-- *Relationship type(en)
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					ELEMENT[at0030] occurrences matches {0..1} matches {	-- *Date of resolution/remission(en)
						value matches {
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0031] occurrences matches {0..1} matches {	-- *Age at resolution/remission(en)
						value matches {
							DV_DURATION matches {*}
						}
					}
					allow_archetype CLUSTER[at0054] occurrences matches {0..*} matches {	-- *Supporting Criteria(en)
						include
							archetype_id/value matches {/.*/}
					}
					allow_archetype CLUSTER[at0055] occurrences matches {0..*} matches {	-- *Clinical Stage/Grade(en)
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0069] occurrences matches {0..1} matches {	-- *Comment(en)
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0074] matches {	-- *Tree(en)
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0075] occurrences matches {0..1} matches {	-- *Link to supporting clinical evidence(en)
						value matches {
							DV_URI matches {*}
						}
					}
					allow_archetype CLUSTER[at0076] occurrences matches {0..*} matches {	-- *Status(en)
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

