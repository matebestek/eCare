definition
	EVALUATION[at0000] matches {	-- *Problem(en)
		data matches {
			ITEM_TREE[at0001] matches {	-- *structure(en)
				items cardinality matches {1..*; ordered} matches {
					ELEMENT[at0002] matches {	-- *Problem(en)
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {	-- *Clinical description(en)
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- *Date of initial onset(en)
						value matches {
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- *Age at initial onset(en)
						value matches {
							DV_DURATION matches {*}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- *Severity(en)
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0006, 	-- *Mild(en)
									at0007, 	-- *Moderate(en)
									at0008]	-- *Severe(en)
								}
							}
						}
					}
					ELEMENT[at0038] occurrences matches {0..1} matches {	-- *Clinical significance(en)
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0010] occurrences matches {0..1} matches {	-- *Date clinically recognised(en)
						value matches {
							DV_DATE matches {
								value matches {yyyy-??-XX}
							}
						}
					}
					ELEMENT[at0037] occurrences matches {0..1} matches {	-- *Age when clinically recognised(en)
						value matches {
							DV_DURATION matches {*}
						}
					}
					CLUSTER[at0011] occurrences matches {0..*} matches {	-- *Location(en)
						items cardinality matches {1..2; ordered} matches {
							ELEMENT[at0012] occurrences matches {0..1} matches {	-- *Body site(en)
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0013] occurrences matches {0..1} matches {	-- *Location description(en)
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					CLUSTER[at0014] occurrences matches {0..*} matches {	-- *Aetiology(en)
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0015] occurrences matches {0..*} matches {	-- *Agent(en)
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0016] occurrences matches {0..1} matches {	-- *Complication of(en)
								value matches {
									DV_TEXT matches {*}
									DV_URI matches {*}
								}
							}
							ELEMENT[at0017] occurrences matches {0..1} matches {	-- *Description(en)
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					CLUSTER[at0018] occurrences matches {0..1} matches {	-- *Occurrences or exacerbations(en)
						items cardinality matches {1..*; ordered} matches {
							ELEMENT[at0019] occurrences matches {0..1} matches {	-- *Frequency of reccurrence(en)
								value matches {
									C_DV_QUANTITY <
									>
								}
							}
							ELEMENT[at0020] occurrences matches {0..1} matches {	-- *Date of last occurrence(en)
								value matches {
									DV_DATE matches {*}
								}
							}
							CLUSTER[at0021] occurrences matches {0..*} matches {	-- *Occurrence/exacerbation(en)
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0022] occurrences matches {0..1} matches {	-- *Clinical description(en)
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0023] occurrences matches {0..1} matches {	-- *Outcome(en)
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0024] occurrences matches {0..1} matches {	-- *Date of onset of occurrence(en)
										value matches {
											DV_DATE matches {*}
										}
									}
									ELEMENT[at0036] occurrences matches {0..1} matches {	-- *Date of resolution of occurrence(en)
										value matches {
											DV_DATE matches {*}
										}
									}
								}
							}
							ELEMENT[at0025] occurrences matches {0..1} matches {	-- *Number of occurrences(en)
								value matches {
									DV_COUNT matches {
										magnitude matches {|>=1|}
									}
								}
							}
						}
					}
					CLUSTER[at0026] occurrences matches {0..1} matches {	-- *Related problems(en)
						items cardinality matches {1..*; unordered} matches {
							CLUSTER[at0027] occurrences matches {0..*} matches {	-- *Related problem(en)
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0028] occurrences matches {0..1} matches {	-- *Related problem(en)
										value matches {
											DV_TEXT matches {*}
											DV_URI matches {*}
										}
									}
									ELEMENT[at0029] occurrences matches {0..1} matches {	-- *Clinical description(en)
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
					ELEMENT[at0030] occurrences matches {0..1} matches {	-- *Date of resolution(en)
						value matches {
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0031] occurrences matches {0..1} matches {	-- *Age at resolution(en)
						value matches {
							DV_DURATION matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0032] matches {	-- *Tree(en)
				items cardinality matches {0..*; unordered} matches {
					CLUSTER[at0033] occurrences matches {0..1} matches {	-- *References(en)
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0034] occurrences matches {0..*} matches {	-- *Reference(en)
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0035] occurrences matches {0..*} matches {	-- *Web link(en)
								value matches {
									DV_URI matches {*}
								}
							}
						}
					}
				}
			}
		}
	}

