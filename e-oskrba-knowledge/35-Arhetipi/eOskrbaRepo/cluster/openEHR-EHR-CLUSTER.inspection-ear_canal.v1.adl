							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0.60, 	-- Ear canal normal
									at0.66]	-- No foreign body
								}
							}
						}
					}
				}
			}
			ELEMENT[at0048] occurrences matches {0..1} matches {	-- Clinical Description
