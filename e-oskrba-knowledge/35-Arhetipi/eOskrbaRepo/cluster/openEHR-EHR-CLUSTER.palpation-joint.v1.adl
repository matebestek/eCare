definition
	CLUSTER[at0000.1] matches {	-- Palpation of joint
		items cardinality matches {1..*; unordered} matches {
			CLUSTER[at0059] occurrences matches {0..1} matches {	-- Normal statements
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0012] occurrences matches {0..*} matches {	-- Normal statement
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0013, 	-- Normal
									at0014, 	-- Probably normal
									at0015, 	-- No abnormality detected
									at0060, 	-- Soft
									at0061, 	-- No tenderness
									at0068, 	-- No masses
									at0069, 	-- No foreign bodies
									at0082]	-- No hernias
								}
							}
						}
					}
				}
			}
			ELEMENT[at0001] occurrences matches {0..1} matches {	-- Clinical description
				value matches {
					DV_TEXT matches {*}
				}
			}
			CLUSTER[at0080] occurrences matches {0..1} matches {	-- Findings
				items cardinality matches {1..*; unordered} matches {
					CLUSTER[at0019] occurrences matches {0..*} matches {	-- Tenderness
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0002] occurrences matches {0..1} matches {	-- Depth of palpation
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0003, 	-- Superficial
											at0004]	-- Deep
										}
									}
								}
							}
							ELEMENT[at0020] occurrences matches {0..1} matches {	-- Degree
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0006, 	-- absent
											at0021, 	-- mild
											at0022, 	-- moderate
											at0023, 	-- marked
											at0024]	-- extreme
										}
									}
								}
							}
							ELEMENT[at0005] occurrences matches {0..1} matches {	-- Category
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0007, 	-- Superficial tenderness
											at0008, 	-- Deep tenderness
											at0009, 	-- Rebound tenderness
											at0019, 	-- Tenderness
											at0025]	-- Rigidity
										}
									}
								}
							}
							use_node ELEMENT /items[at0080]/items[at0010]/items[at0011]	-- /items[Findings]/items[Localised palpation]/items[Body site]
						}
					}
					ELEMENT[at0041] occurrences matches {0..1} matches {	-- Temperature
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0042, 	-- Cold
									at0043, 	-- Cool
									at0044, 	-- Normal
									at0045, 	-- Warm
									at0046]	-- Hot
								}
							}
						}
					}
					CLUSTER[at0070] occurrences matches {0..1} matches {	-- Guarding
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0071] occurrences matches {0..1} matches {	-- Degree
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0076, 	-- Not present
											at0077, 	-- Mild
											at0078, 	-- Moderate
											at0079]	-- Gross
										}
									}
								}
							}
							ELEMENT[at0072] occurrences matches {0..1} matches {	-- Nature
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0073, 	-- Voluntary
											at0074, 	-- Involuntary
											at0075]	-- Uncertain
										}
									}
								}
							}
						}
					}
					CLUSTER[at0010] occurrences matches {0..*} matches {	-- Localised palpation
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0011] matches {	-- Body site
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0.83] occurrences matches {0..1} matches {	-- Deformity
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {[local::]}
									}
								}
							}
							allow_archetype CLUSTER[at0083] occurrences matches {0..2} matches {	-- Cluster: Localised palpation
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.palpation\.v1/}
								exclude
									archetype_id/value matches {/.*/}
							}
							ELEMENT[at0081] occurrences matches {0..1} matches {	-- Cough impulse
								value matches {
									DV_BOOLEAN matches {
										value matches {True}
									}
								}
							}
						}
					}
					CLUSTER[at0053] occurrences matches {0..1} matches {	-- Surface/Border
						name matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0064, 	-- Surface
									at0065]	-- Edge
								}
							}
						}
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0067] occurrences matches {0..1} matches {	-- Site description
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0048] occurrences matches {0..1} matches {	-- Consistency
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0049, 	-- Hard/Rigid
											at0050, 	-- Firm
											at0052, 	-- Soft
											at0062, 	-- Boggy
											at0063]	-- Indents
										}
									}
								}
							}
							ELEMENT[at0054] occurrences matches {0..*} matches {	-- Features
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0055, 	-- Smooth
											at0056, 	-- Craggy
											at0057, 	-- Irregular
											at0058]	-- Nodular
										}
									}
								}
							}
						}
					}
					ELEMENT[at0.93] occurrences matches {0..1} matches {	-- Crepitus present
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					ELEMENT[at0.94] occurrences matches {0..1} matches {	-- Locking of joint present
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					CLUSTER[at0.85] occurrences matches {0..1} matches {	-- Effusion
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0.86] occurrences matches {0..1} matches {	-- Present
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
							ELEMENT[at0.87] occurrences matches {0..1} matches {	-- Description
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {[local::]}
									}
								}
							}
							ELEMENT[at0.88] occurrences matches {0..1} matches {	-- Degree
								value matches {
									0|[local::at0.89], 	-- Absent
									1|[local::at0.90], 	-- Mild
									2|[local::at0.91], 	-- Moderate
									3|[local::at0.92]; 	-- Large
									0  	-- assumed value
								}
							}
						}
					}
					ELEMENT[at0.84] occurrences matches {0..1} matches {	-- Comparison to other side
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0.95] occurrences matches {0..*} matches {	-- Specific Test
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0.96] occurrences matches {0..1} matches {	-- Name of test
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {[local::]}
									}
								}
							}
							ELEMENT[at0.97] occurrences matches {0..1} matches {	-- Outcome of test
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {[local::]}
									}
								}
							}
							ELEMENT[at0.98] occurrences matches {0..1} matches {	-- Comments
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {[local::]}
									}
								}
							}
						}
					}
					CLUSTER[at0047] occurrences matches {0..1} matches {	-- Detail
						items cardinality matches {1..*; unordered} matches {
							allow_archetype CLUSTER[at0084] occurrences matches {0..1} matches {	-- Cluster: Exam details
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.exam\.v1|openEHR-EHR-CLUSTER\.exam-mass\.v1/}
								exclude
									archetype_id/value matches {/.*/}
							}
						}
					}
				}
			}
		}
	}


